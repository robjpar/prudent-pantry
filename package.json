{
  "name": "prudent-pantry",
  "version": "1.0.0",
  "description": "MERN Stack Web App",
  "main": "server.js",
  "scripts": {
    "install": "cd client && npm install",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "build": "cd client && npm run build",
    "prod": "cross-env NODE_ENV=production npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robjpar/prudent-pantry.git"
  },
  "keywords": [
    "react",
    "mongo",
    "express",
    "node"
  ],
  "author": "David LiaBraaten, Mohsen Shojaeifar, Robert Paruch & Tyler Jeffe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robjpar/prudent-pantry/issues"
  },
  "homepage": "https://github.com/robjpar/prudent-pantry#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "ip": "^1.1.5",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.7.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-fontawesome": "^1.6.1",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "nodemon": "^1.19.1"
  }
}
